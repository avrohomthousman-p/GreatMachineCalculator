  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Xamarin.AndroidX.AppCompat" Version="1.7.0.5" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\GreatMachineCalculator\GreatMachineCalculator.csproj" />
  </ItemGroup>
</Project>